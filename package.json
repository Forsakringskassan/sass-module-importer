{
  "name": "@forsakringskassan/sass-module-importer",
  "version": "1.1.6",
  "description": "sass module importer",
  "keywords": [
    "sass",
    "importer"
  ],
  "homepage": "https://github.com/Forsakringskassan/sass-module-importer",
  "bugs": "https://github.com/Forsakringskassan/sass-module-importer/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Forsakringskassan/sass-module-importer.git"
  },
  "license": "MIT",
  "author": "Försäkringskassan",
  "type": "module",
  "exports": {
    ".": {
      "types": "./importer.d.ts",
      "default": "./dist/importer.js"
    }
  },
  "types": "./importer.d.ts",
  "files": [
    "dist",
    "importer.d.ts"
  ],
  "scripts": {
    "build": "node build.mjs",
    "eslint": "eslint .",
    "eslint:fix": "eslint --fix .",
    "lint": "run-s eslint prettier:check",
    "lint:fix": "run-s eslint:fix prettier:write",
    "prettier:check": "prettier --check .",
    "prettier:write": "prettier --write .",
    "pretest": "npm run lint",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "commitlint": {
    "extends": "@forsakringskassan/commitlint-config/no-jira"
  },
  "prettier": "@forsakringskassan/prettier-config",
  "release": {
    "extends": "@forsakringskassan/semantic-release-config"
  },
  "dependencies": {
    "resolve-package-path": "^4.0.3",
    "resolve.exports": "^2.0.3"
  },
  "devDependencies": {
    "@forsakringskassan/commitlint-config": "3.3.3",
    "@forsakringskassan/eslint-config": "13.3.2",
    "@forsakringskassan/eslint-config-cli": "13.3.2",
    "@forsakringskassan/eslint-config-jest": "13.3.3",
    "@forsakringskassan/jest-config": "30.1.1",
    "@forsakringskassan/prettier-config": "3.3.4",
    "esbuild": "0.27.3",
    "npm-pkg-lint": "4.3.4",
    "npm-run-all2": "8.0.4",
    "sass": "1.97.3"
  },
  "peerDependencies": {
    "sass": "^1.79.0"
  },
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=9.0.0"
  }
}
